#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/ext_power.h>

#define DEF_L 0

/ {
    behaviors {
        fn_btns: function_buttons {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&studio_unlock>, <&ext_power EP_TOG>, <&bt BT_CLR>;
        };
    };

    combos {
        compatible = "zmk,combos";
        fn_combo {
            timeout-ms = <50>;
            key-positions = <0 1>;
            bindings = <&fn_btns>;
        };
    };

    keymap {
        compatible = "zmk,keymap";
        default_layer {
            bindings = <
                &kp C_VOL_DN    &kp C_VOL_UP
            >;
        };

    };
};
